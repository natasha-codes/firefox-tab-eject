{
  "name": "firefox-tab-eject",
  "version": "0.0.1",
  "description": "A simple tool to pop the current tab out to a new window via user-customizable shortcut (defaults to Ctrl-Shift-w)",
  "repository": "git@github.com:natasha-codes/firefox-tab-eject.git",
  "author": "natasha-codes <https://github.com/natasha-codes>",
  "contributors": [
    "Nathan Shelly (https://nathanshelly.dev)",
    "Sasha Weiss (https://asashaweiss.com)"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc",
    "format": "prettier --write \"**/*.{json,ts,md}\"",
    "package": "web-ext build --artifacts-dir dist --overwrite-dest"
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "^78.0.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "web-ext": "^4.3.0"
  }
}
